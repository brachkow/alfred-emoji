{
  "name": "alfred-emoji",
  "description": "Alfred workflow for searching and copying emojis",
  "main": "dist/emoji-search.js",
  "scripts": {
    "build": "node build.js",
    "dev": "pnpm build && cp -r dist/* ~/Library/Application\\ Support/Alfred/Alfred.alfredpreferences/workflows/user.workflow.*/",
    "package": "pnpm build && cd dist && zip -r ../alfred-emoji.alfredworkflow .",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "keywords": [
    "alfred",
    "emoji",
    "workflow",
    "search"
  ],
  "packageManager": "pnpm@10.7.1",
  "dependencies": {
    "emojibase": "^16.0.0",
    "emojibase-data": "^16.0.3",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.5.2",
    "esbuild": "^0.25.10",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
